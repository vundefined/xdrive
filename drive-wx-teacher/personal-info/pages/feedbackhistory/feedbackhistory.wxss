@import "/styles/nodata.wxss";

.feed-back {
	width: 710rpx;
	box-sizing: border-box;
	margin: 20rpx 20rpx 20rpx 20rpx;
	background-color: #fff;
	border-radius: 10rpx;
	padding: 20rpx 20rpx 20rpx 20rpx;
}

.feed-back-num {
	padding: 10rpx 0rpx 30rpx 0rpx;
	border-bottom: 2rpx solid #eeeeee;
}

.feed-back-content {
	margin: 40rpx 0rpx 40rpx 0rpx;
}

.feed-back-img {
	width: 147.5rpx;
	height: 147.5rpx;
	margin: 0rpx 10rpx;
}